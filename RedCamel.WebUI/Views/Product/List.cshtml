@model RedCamel.WebUI.Models.ProductsListViewModel

@{
    ViewBag.Title = "Products";
}

@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary", p);
}


<div class="btn-group pull-right ">
    @Html.PageLinks(Model.PagingInfo,z=>Url.Action("List",new {page = z, category=Model.CurrentCategory}))
</div>

